<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <!-- Bootstrap CSS -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <link rel="shortcut icon" type="image/jpg" href="/images/favicon.png">
        <title><%= pageTitle  %></title>
    </head>
    <body>
        <div class="container">
            <h1 class="mt-5 mb-5 text-center text-primary"><b>Chi tiet ban va doanh thu</b></h1>
            
            <div>
                <label class="text-black-50">Table ID: </label>
                <span><%= tableId %></span>
            </div>
            <div>
                <label class="text-black-50">Ngay bat dau: </label>
                <span><%= startDate %></span>
            </div>
            <div>
                <label class="text-black-50">Gio bat dau: </label>
                <span><%= startTime %></span>
            </div>
            <div>
                <label class="text-black-50">Ngay ket thuc: </label>
                <span><%= endDate %></span>
            </div>
            <div>
                <label class="text-black-50">Gio ket thuc: </label>   
                <span><%= endTime %></span>
            </div>
            <hr class="my-2 border border-black">
            <div>
                <label class="text-black-50">Doanh thu: </label>
                <span><%= doanhThu %></span>
            </div>

        </div>
    </body>
</html>