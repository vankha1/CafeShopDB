<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <title>
        <%= pageTitle %>
    </title>
</head>

<body>
    <div class="container">
        <h1 class="mt-5 mb-5 text-center text-primary"><b>Chi tiết bàn & Doanh thu</b></h1>

        <div>
            <label class="text-black-50">Mã bàn: </label>
            <span>
                <%= tableId %>
            </span>
        </div>
        <div>
            <label class="text-black-50">Ngày bắt đầu: </label>
            <span>
                <%= startDate %>
            </span>
        </div>
        <div>
            <label class="text-black-50">Giờ bắt đầu: </label>
            <span>
                <%= startTime %>
            </span>
        </div>
        <div>
            <label class="text-black-50">Ngày kết thúc: </label>
            <span>
                <%= endDate %>
            </span>
        </div>
        <div>
            <label class="text-black-50">Giờ kết thúc: </label>
            <span>
                <%= endTime %>
            </span>
        </div>
        <hr class="my-2 border border-black">
        <div>
            <label class="text-black-50">Doanh thu: </label>
            <span>
                <%= doanhThu %>
            </span>
        </div>

    </div>
</body>

</html>